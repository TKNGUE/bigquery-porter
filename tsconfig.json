{
 "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "incremental": true, 

    "target": "esnext",
    "moduleResolution": "node",

    "module": "esnext",
    "paths": {
        "*": ["src/*.d.ts"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
